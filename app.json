{
  "expo": {
    "name": "menus",
    "description": "Sample app for restaurants' menus",
    "slug": "menues_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/menu_icon.png",
    "splash": {
      "image": "./assets/menu.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },

    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
